<request>
	<unique id></unique id>//client make it,server return the result file by name the file of this id
	<data type = "group">
		<operation type = "list">
			<condition name = "sort" value = "name"></condition>
		</operation>
		<operation type = "add">
			<content>
				<name></name>
				<ring></ring>
			</content>
		</operation>
		<operation type = "delete">
			<content>
				<id></id>
			</content>
		</operation>
		<operation type = "edit">
			<content>
				<id></id>//server make it,client get it by list info
				<name></name>
				<ring></ring>
			</content>
		</operation>
	</data>
	
	<data type = "contactors">
		<operation type = "list" kind = "phoneno">
			<condition name = "sort" value = "name"></condition>
		</operation>
		<operation type = "add">
			<id></id>
			<content>
				<basicinfo>
					<name></name>
					<ring></ring>
					<birthday></birthday>
					<icon></icon>
				</basicinfo>
				<phone>
					<default>key-id</default>
					<key value = ""></key>
				</phone>
				<function>
					<notifybirthday></notify birthday>
					<notifymiss></notifymiss>
				</function>
			</content>
		</operation>
		<operation type = "delete" kind = "phoneno">
			<id></id>
			<key>key-id</key>
		</operation>
		<operation type = "edit">
			<id></id>//server make it,client get it by list info
			<content>
				<basicinfo>
					<name></name>
					<ring></ring>
					<birthday></birthday>
					<icon></icon>
					<group></group>
				</basicinfo>
				<phone>
					<default>key-id</default>
					<key id = "" value = ""></key>
				</phone>
				<function>
					<notify birthday></notify birthday>
					<notifymiss></notifymiss>
				</function>
			</content>
		</operation>
		<operation type = "detail">
			<id></id>//server make it,client get it by list info
		</operation>
	</data>
	
	<data type = "owner">
		<data type = "favorite">
			<operation type = "list">
				<condition name = "sort" value = "time"></condition>
			</operation>
			<operation type = "detail">
				<id></id>
			</operation>
		</data>
	</data>

</request>